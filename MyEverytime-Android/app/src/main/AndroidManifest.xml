<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myeverytime">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyEverytime"
        android:usesCleartextTraffic="true">
        <activity android:name=".MainActivityForFragment"></activity>
        <activity android:name=".main.boards.updating.UpdatingActivity" />
        <activity android:name=".main.boards.in_post.InPostActivity" />
        <activity android:name=".main.boards.writing.WritingActivity" />
        <activity android:name=".main.boards.freeboard.FreeBoardActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".signUp.SignUpInputFormActivity" />
        <activity android:name=".signIn.SignInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>